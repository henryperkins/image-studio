{
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["react", "react-dom"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@image-studio/shared": ["../shared/src"]
    }
  },
  "exclude": ["src/__tests__/**/*"]
}